<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee" version="5.0">
    <servlet>
        <servlet-name>liste</servlet-name>
        <servlet-class>com.example.servlet.ListeEtudiantsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>liste</servlet-name>
        <url-pattern>/etudiants</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UtilisateurServlet</servlet-name>
        <url-pattern>/utilisateurs</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>EmpruntServlet</servlet-name>
        <servlet-class>com.example.servlet.EmpruntServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmpruntServlet</servlet-name>
        <url-pattern>/emprunts</url-pattern>
    </servlet-mapping>
      <welcome-file-list>
        <welcome-file>emprunts.jsp</welcome-file>
    </welcome-file-list>
</web-app>